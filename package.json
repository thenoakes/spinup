{
  "name": "spinup",
  "version": "1.0.0",
  "author": "Matt Noakes @thenoakes",
  "bin": {
    "spinup": "./bin/run"
  },
  "bugs": "https://github.com/thenoakes/spinup/issues",
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@types/chalk": "^2.2.0",
    "chalk": "^4.1.1",
    "tmp": "^0.2.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.19",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@types/tmp": "^0.2.1",
    "chai": "^4.3.4",
    "eslint": "^8.22.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/thenoakes/spinup",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "spinup",
    "macos": {
      "identifier": "com.thenoakes.spinup"
    }
  },
  "repository": "thenoakes/spinup",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "build": "rm -rf lib; tsc -b",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "pack": "npm run build && oclif-dev pack:macos"
  },
  "types": "lib/index.d.ts",
  "mocha": {
    "extension": [
      "ts"
    ],
    "package": "./package.json",
    "recursive": true,
    "reporter": "spec",
    "require": "ts-node/register",
    "timeout": "5000",
    "ui": "bdd",
    "watch-files": [
      "test/**/*.ts"
    ]
  }
}
